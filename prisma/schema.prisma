datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model product {
  id       String    @id @default(auto()) @map("_id") @db.ObjectId
  title    String
  body     String?
  price    Float
  image    String?
  createdAt DateTime @default(now())
  }